    "@stripe/stripe-js": "^4.1.0",
    "@vueform/slider": "^2.1.10",
    "@vueuse/core": "^10.11.0",
    "unplugin-auto-import": "^0.17.1",
    "unplugin-vue-components": "^0.27.4"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
